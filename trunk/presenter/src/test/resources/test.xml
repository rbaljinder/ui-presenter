<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:util="http://www.springframework.org/schema/util"
	xmlns:ic="http://www.rbaljinder.org/schema/presenter" xmlns:aop="http://www.springframework.org/schema/aop"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.0.xsd
		http://www.springframework.org/schema/util http://www.springframework.org/schema/util/spring-util-2.0.xsd
		http://www.springframework.org/schema/aop http://www.springframework.org/schema/aop/spring-aop-2.0.xsd
		http://www.rbaljinder.org/schema/presenter presenter.xsd">
	<ic:data-control name="Workbench_SalesAgentChannelMapping_SalesChannelMapping_DataControl" coded-value="true" scope="prototype" access-strategy="direct"
		event-handler="DoNothingEventHandler">
		<ic:model class="SystemX" />
	</ic:data-control>
	<ic:data-control name="DataSourceResponses_CurrentApplicationData_Data_AppSummary" event-handler-ref="testEventHandler">
		<ic:model class="org.baljinder.presenter.ui.model.Subject" />
		<ic:model class="org.baljinder.presenter.ui.model.DataSourceResponse" />
		<ic:model class="org.baljinder.presenter.ui.model.DataSourceProductResponse" />
		<ic:parent-relations>
			<ic:parent-relation relation="subject.subjectId = subject.subjectId" />
		</ic:parent-relations>
		<ic:join-criteria
			criteria=" dataSourceResponse.dataSourceResponseId = dataSourceProductResponse.dataSourceResponseId and subject.subjectId =  dataSourceResponse.subjectId" />

	</ic:data-control>
	<ic:transition name="testTransition" scope="prototype" event-handler-ref="testEventHandler">
		<ic:target-page page="testPage" />
		<ic:transition-action class="DoNothingTransitionAction" scope="prototype" />
	</ic:transition>
	<ic:event-handler name="testEventHandler" class="DoNothingEventHandler" />
	<ic:transition name="testTransition1" scope="prototype">
		<ic:target-page page="testPage" />
		<ic:transition-action class="DoNothingTransitionAction" scope="prototype" />
	</ic:transition>
	<ic:page name="testPage" scope="prototype" event-handler-ref="testEventHandler">
		<ic:data-control name="Workbench_DataWarehousePreference_DIInteractionConfig_DataControlX" init-method="initialize" scope="prototype" size="20"
			access-strategy="jsf-based">
			<ic:type class="org.baljinder.presenter.dataacess.internal.DataControl" />
			<ic:default-where-clause clause="testClause" />
			<ic:dao-key-name key="org.baljinder.presenter.dataacess.internal.GenericPresentationDAOImpl" />
			<ic:persistence-manager manager="presentationPersistence" />

			<ic:query-builder builder="defaultQueryBuilder" />
			<ic:model class="SystemX" />
			<ic:join-criteria criteria="testC" />
			<ic:data-control name="Workbench_DataWarehousePreference_System_DataControlxx" access-strategy="direct" scope="prototype">
				<ic:model class="SystemX" />
				<ic:parent-relations>
					<ic:parent-relation relation="testRelation" />
					<ic:parent-relation relation="testRelation1" />
				</ic:parent-relations>
			</ic:data-control>
		</ic:data-control>
	</ic:page>
	<!--
		<ic:transition name="FindApplication_AppSummary_Transition"> <ic:target-page page="FindApplication_AppSummary_Controller" /> <ic:sourceDataControl
		dataControl="SearchApplication_FindApplication_Data_AppSummary"/> <ic:targetDataControl dataControl="Application_CurrentApplicationData_Data_AppSummary"/>
		<ic:transitionCriteria> <ic:transitionCriterion criterion="transaction.transactionId = transaction.transactionId"/> </ic:transitionCriteria> </ic:transition>

		<ic:page name="Workbench_DataWarehousePreference_Controller" cached="true" scope="prototype"> <ic:type
		class="org.baljinder.presenter.dataacess.internal.PageController" /> <ic:data-control
		name="Workbench_DataWarehousePreference_DIInteractionConfig_DataControl" init-method="initialize" scope="prototype" size="20"> <ic:type
		class="org.baljinder.presenter.dataacess.internal.DataControl" /> <ic:default-where-clause clause="testClause" /> <ic:dao-key-name
		key="org.baljinder.presenter.dataacess.internal.GenericPresentationDAOImpl" /> <ic:persistence-manager manager="presentationPersistence" />

		<ic:query-builder builder="defaultQueryBuilder" /> <ic:model class="org.baljinder.presenter.ui.model.System" /> <ic:model
		class="org.baljinder.presenter.ui.model.DiInteractionConfig" /> <ic:join-criteria criteria="testC" /> <ic:data-control
		name="Workbench_DataWarehousePreference_System_DataControlX"> <ic:model class="org.baljinder.presenter.ui.model.DiInteractionConfig" />
		<ic:parent-relations> <ic:parent-relation relation="testRelation" /> <ic:parent-relation relation="testRelation1" /> </ic:parent-relations> </ic:data-control>
		</ic:data-control> <ic:data-control name="Workbench_DataWarehousePreference_System_DataControl" scope="prototype"> <ic:model
		class="org.baljinder.presenter.ui.model.DiInteractionConfig" /> </ic:data-control> </ic:page> <ic:transition
		name="xFindApplication_AppSummary_Transition" mode="query"> <ic:target-page page="FindApplication_AppSummary_Controller" /> </ic:transition>
	-->
	<bean id="presentationPersistence" class="org.baljinder.presenter.dataacess.PersistenceServiceWrapper">
		<!--
			<property name="persistenceContexts"> <map> <entry key="testContext" value-ref="presentation.platform.persistence.testContext" /> </map> </property>
		-->
		<property name="daoAssociations">
			<map>
				<entry key="org.baljinder.presenter.dataacess.internal.extension.ValidValueGenericDaoImpl" value="genericValidValueDao" />
				<entry key="org.baljinder.presenter.dataacess.internal.GenericPresentationDAOImpl" value="genericPresentationDao" />
				<entry key="org.baljinder.presenter.dataacess.cvl.CodedValueDAO" value="codedValueDAO" />
			</map>
		</property>
	</bean>
	<bean id="defaultQueryBuilder" class="org.baljinder.presenter.dataacess.util.BasicQueryBuilder">
		<property name="filters" ref="dataControlFilters" />
	</bean>

	<bean id="jsfBasedDataAccessStrategy" class="org.baljinder.presenter.dataacess.internal.support.JSFSupportingDataAccessStrategy" />

	<bean id="directDataAccessStrategy" class="org.baljinder.presenter.dataacess.internal.support.DirectDataAccessStrategy" />

	<bean id="systemIdFilter" class="org.baljinder.presenter.dataacess.filter.SystemIdFilter" />

	<util:map id="dataControlFilters">
		<entry key="Workbench_BureauSwitch_System_DataControl">
			<map>
				<entry key="org.baljinder.presenter.ui.model.System" value-ref="systemIdFilter" />
			</map>
		</entry>
		<entry key="Workbench_DataWarehousePreference_System_DataControl">
			<map>
				<entry key="org.baljinder.presenter.ui.model.System" value-ref="systemIdFilter" />
			</map>
		</entry>
		<entry key="AppSummary_FindApplication_SearchApplication_DataControl">
			<map>
				<entry key="org.baljinder.presenter.ui.model.System" value-ref="systemIdFilter" />
			</map>
		</entry>

	</util:map>
</beans>