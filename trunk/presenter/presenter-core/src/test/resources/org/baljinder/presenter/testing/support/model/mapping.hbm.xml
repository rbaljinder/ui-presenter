<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 2.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">
<hibernate-mapping>
	<class name="org.baljinder.presenter.testing.support.model.DomainModel"
		table="test">
		<id name="testId" column="id">
			<generator class="assigned"></generator>
		</id>
		<property name="name" />
	</class>
	<class name="org.baljinder.presenter.testing.support.model.AnotherTestTable"
		table="anotherTest">
		<id name="testId" column="id">
			<generator class="assigned"></generator>
		</id>
		<property name="name" />
	</class>
	<class name="org.baljinder.presenter.testing.support.model.System">
		<id name="systemId">
			<generator class="assigned"></generator>
		</id>
		<property name="systemName" />
		<property name="clientId" />
	</class>
	<class name="org.baljinder.presenter.testing.support.model.Client">
		<id name="clientId">
			<generator class="assigned"></generator>
		</id>
		<property name="clientName" />
	</class>
</hibernate-mapping>
